#:import Factory kivy.factory.Factory


<Manager>:
    orientation:'vertical'
    # Widget dos botões do top
    BoxLayout:
        size_hint_y:.2
        padding:12
        spacing:11
        canvas:
            Color:
                rgba:.0,.33,.42,.5
            Rectangle:
                size:self.width, self.height - 8
                pos:self.x, self.y + 7
            Color:
                rgba:.23,.33,.12,1
            Rectangle:
                size:self.width,3
                pos:self.pos
        Button:
            size_hint_x:None
            width:'50dp'
            text:'Inicial'
            on_release:manager.current = 'screenimage'
        MDRectangleFlatIconButton:
            size_hint_y:1
            text:'Login'
            font_size:'30dp'
            bold:True
            icon:'login'
            on_press:manager.transition = Factory.get('FadeTransition')()
            on_release:manager.current = 'screenlogin'

        MDRectangleFlatIconButton:
            size_hint_y:1
            icon:'account'
            text:'Cadastro'
            bold:True
            font_size:'30dp'
            on_press:manager.transition = Factory.get('WipeTransition')()
            on_release:manager.current = 'screenregister'

        MDRectangleFlatIconButton:
            size_hint_y:1
            icon:'login'
            font_size:'30dp'
            bold:True
            text:'Serviços'
            on_press:manager.transition = Factory.get('SwapTransition')()
            on_release:manager.current = 'screenview'
        MDRectangleFlatIconButton:
            size_hint_y:1
            icon:'login'
            font_size:'30dp'
            bold:True
            text:'Estoque'
            on_press:manager.transition = Factory.get('FadeTransition')()
            on_release:manager.current = 'screenestoque'

    ScreenManager:
        id:manager
        canvas:
            Color:
#                rgba:0,.1,.2,.1
                rgba:1,1,1,.7
            Rectangle:
                size:self.width, self.height - 8
                pos:self.x, self.y + 8
            Color:
                rgba:.23,.33,.12,1
            Rectangle:
                size:self.width, 3
                pos:self.pos

#        ScreenImage:
#            name:'screenimage'
#        ScreenRegister:
#            name:'screenregister'
#        ScreenLogin:
#            name:'screenlogin'
#        ScreenView:
#            name:'screenview'
        ScreenEstoque
            name:'screenestoque'



<ScreenImage>:
    BoxLayout:
        size_hint_x:1
        padding:20
        Image:
            size_hint_x:self.width + 100
            source:'image/image.jpg'
            allow_stretch:True

<ScreenLogin>:
# === Tela Login
    FloatLayout:
        BoxLayout:
            size_hint_y:None
            height:'20dp'
            pos_hint:{'x':.0,'center_y':.93}
            canvas:
                Color:
                    rgba:1,1,1,0
                Rectangle:
                    size:self.size
                    pos:self.pos
                Color:
                    rgba:.5,0,0,5
                Rectangle:
                    size:self.width,2
                    pos:self.pos
            MDLabel:
                pos_hint:{'center_y':.95}
                italic:True
#                outline_width:1
#                outline_color:0,0,0,1
                text:'Tela Login Clientes'
                font_size:'35dp'
                color:.5,0,0,5
# ===== Nome
        FloatLayout:
            MDCard:
                orientation:'vertical'
                md_bg_color:1,1,1,.2
                elevation:2
                size_hint:None,None
                width:'650dp'
                height:'450dp'
                pos_hint:{'center_x':.5,'center_y':.5}
                radius:11,11,11,11
                FloatLayout:
                    id:float_layout
                    MDLabel:
                        size_hint_x:None
                        width:'1000dp'
                        halign:'center'
                        pos_hint:{'center_y':.7}
                    MDTextField:
                        size_hint_x:None
                        width:'200dp'
                        mode:'rectangle'
                        hint_text:'CPF:'
                        font_size:'30sp'
                        max_text_length:11
#                        color_mode:'custom'
                        line_color_focus:0,0,0,1
                        pos_hint:{'center_x':.5,'center_y':.7}
    #                    line_color_normal: app.theme_cls.accent_color
                        id:login_cpf
                    MDFillRoundFlatIconButton:
                        pos_hint:{'center_x':.5,'center_y':.55}
                        halign:'center'
                        text:'Limpar'
                        icon:'api-off'
                        on_release:root.ids.login_cpf.text = ''
                    MDLabel:
                        id:warning
                        halign:'center'
                        pos_hint:{'center_x':.5}
                        font_size:'35sp'
                        color: .4,0,0,1


            MDFillRoundFlatIconButton:
                pos_hint:{'right':.97,'center_y':.1}
                text:'Pesquisar'
                icon:'account-check'
#                on_release:root.get_data_cliente()
                on_release:root.content_base()

