                width:'300dp'
                height:'30dp'
                spacing:'13dp'
                MDLabel:
                    text:'Valor/soma'
                    color:0,0,0,1
                MDTextFieldRect:
                    hint_text:'0.0 R$'
                MDLabel:
                    size_hint_x:.3
                    color:0,0,0,1
                    text:'R$'
