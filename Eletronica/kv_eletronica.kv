#                outline_width:1
#                outline_color:0,0,0,1
                text:'Tela Login Clientes'
                font_size:'35dp'
                color:.5,0,0,5
# ===== Nome
        FloatLayout:
            MDCard:
                orientation:'vertical'
                md_bg_color:1,1,1,.2
                elevation:2
                size_hint:None,None
                width:'650dp'
                height:'450dp'
                pos_hint:{'center_x':.5,'center_y':.5}
                radius:11,11,11,11
